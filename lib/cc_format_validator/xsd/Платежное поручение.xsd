<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://bssys.com/sbns/integration" targetNamespace="http://bssys.com/sbns/integration" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PayDocRu">
		<xs:annotation>
			<xs:documentation>Рублевое Платежное Поручение. Представление в БД.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="absAcceptDate" minOccurs="0" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Время принятия электронного документа в АБС банка</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="acceptIndividual" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Признак подтверждения пользователем, что платеж не связан с предпренемательской деятельностью</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="bankAcceptDate" minOccurs="0" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Дата принятия электронного документа в банке (поступило в банк плательшика)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="bankMessage" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сообщение из банка</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="bankMessageAuthor" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ответственный исполнитель</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="1024"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="branchExtId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор подразделения в АБС</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="budgetaryPayment" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Признак бюджетный платеж</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="cbc" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код КБК</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="chargeType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип налогового платежа/Код выплат</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="creditContract" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер кредитного договора</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="currencyOperationType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код вида валютной операции</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="dealPassport" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер паспорта сделки</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="30"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="deliveredTime" minOccurs="0" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Дата и время принятия электронного документа в банке (поступило в банк плательшика)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="docDate" minOccurs="0" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата документа Берется как на календаре у пользователя, в том же виде хранится в базе. То есть если в москве 23 часа, в красноярске 4 часа. То созданная платежка будет иметь в базе дату создания 4 часа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="docDateDay" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата налогового документа (день)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="docDateMonth" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата налогового документа (месяц)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="docDateYear" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата налогового документа (год)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="docId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор документа</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="36"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="docNumber" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер документа</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="documentSum" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сумма документа, которым было сформирована данная операция</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="18"/>
							<xs:fractionDigits value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="drawerStatus" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Показатель статуса</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="extSystemCode" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор ОБ</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="externalId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор документа во внешней системе</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="externalUPGId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор документа во внешней системе для UPG</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="forAccept" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Признак документа 'К акцепту'</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="globalId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Внешний идентификатор документа МБА.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="individual1Name" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Фамилия получателя</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="355"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="individual2Name" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Имя получателя</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="355"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="individual3Name" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Отчество получателя</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="355"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="individualOrLegal" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Получатель Физ лицо/юр лицо.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="lastModifyDate" minOccurs="0" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Дата последней модификации документа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="mbaAccountId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор счета плательщика МБА.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mbaActionName" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Имя действия МБА.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mbaCreateDate" minOccurs="0" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Дата и время создания документа МБА.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="mbaDoc" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Признак документа МБА.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mbaDocId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор документа МБА.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mbaStateName" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дополнительный статус МБА.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="messageFA" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сообщение от FA (фрод-анализа)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="messageFAAuthor" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ответственный исполнитель FA (фрод-анализа)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="messageOnlyForBank" minOccurs="0" type="xs:string">
					<xs:annotation>
						<xs:documentation>Сообщнение от АБС для банка</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="mobilePaymentType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип платежа (Себе, В бюджет, Контрагенту)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mpId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор массового платежа</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="notifyByEmail" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Оповестить по e-mail</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="notifyBySMS" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Оповестить по SMS</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="obDocId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор документа в ОБ</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ocato" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код ОКТМО</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="oldDocId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор старого документа Используется при импорте документа (в частности, в случае переноса документа из Correqts в данном поле будет хранится GLOBALDOCID из таблицы ABSTRACTDOC)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="operationDate" minOccurs="0" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата исполнения документа. Выставляется при переходе на статус "Исполнен"</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="operationType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Вид операции</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="orgExtId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор организации в АБС</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="orgLegacyId" minOccurs="0" type="xs:int">
					<xs:annotation>
						<xs:documentation>Идентификатор организации в ДБО3</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="orgName" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Наименование организации автора документа По данному полю устанавливается связь с организацией хранит name организации</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="355"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="orgType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип организации</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="JURIDICAL"/>
							<xs:enumeration value="SOLE_ENTREPRENEUR"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payReason" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Основание платежа</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="2"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payToBusinessAcc" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Признак зачисления на бизнес-счет</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:boolean">
							<xs:pattern value="[0-1]{1}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payUntil" minOccurs="0" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>Срок платежа</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="payerAccount" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Счет плательщика</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payerBankBic" minOccurs="0">
					<xs:annotation>
						<xs:documentation>БИК банка плательщика</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="64"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payerBankCity" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Населенный пункт банка плательщика</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payerBankCorrAccount" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Кор. счет банка плательщика</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="payerBankName" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Наименование банка плательщика</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="770"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
